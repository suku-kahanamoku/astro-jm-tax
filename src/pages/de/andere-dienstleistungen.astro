---
import { t } from 'i18next';

import Layout from '@layouts/Layout.astro';
import Intro from '@components/content/Intro.astro';
import Contact from '@components/content/Contact.astro';
import OtherCmp from '@components/content/OtherCmp.astro';
import IntroImage from 'assets/img/bookkeeping.jpg';
---

<Layout title={t('$.base.title')} description={t('$.other.description')}>
	<Intro title="$.other.title" description="$.other.description" image={IntroImage} />
	<OtherCmp />
	<Contact />
</Layout>
